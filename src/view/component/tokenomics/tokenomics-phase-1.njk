        <div class="tokenomics__presentation section__cell">
            <div class="tokenomics__imgbox">
                {% for item in prize.prize_list %}
                    <img src="/img/regular/coffin/coffin_{{ item.img }}.png"
                         alt="{{ item.img }}"
                         data-id="{{ loop.index }}"
                         class="img tokenomics__img-anim{{ ' tokenomics__img-anim_active' if item.class_active else '' }}">
                {% endfor %}
            </div>
        </div>
    </div>
